{
  "name": "ajta30/laravellimesurveyapi",
  "description": "Laravel wrapper for the LimeSurvey RemoteControl2 API",
  "keyword": [
    "limesurvey",
    "laravel-limesurvey",
    "limesurvey-api",
    "json-RPC",
    "limesurvey-jsonRPC"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "ajta30",
      "email": "andres.toyo@ada-lab.cl"
    },
      {
      "name": "du-lei",
      "email": "jsoria@ada-lab.cl"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "ajta30\\laravellimesurveyapi\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "require": {
    "weberhofer/jsonrpcphp": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "weberhofer/jsonrpcphp": "^2.2"
  },
  "extra": {
    "laravel": {
      "providers": [
        "ajta30\\laravellimesurveyapi\\LaravelLimesurveyApiProvider"
      ],
      "aliases": {
      	"LimeSurveyAPI": "ajta30\\laravellimesurveyapi\\Facades\\LimeSurveyAPI"
      }
    }
  }
}
