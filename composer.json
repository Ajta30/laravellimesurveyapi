{
  "name": "neilherbertuk/laravellimesurveyapi",
  "description": "Laravel wrapper for the LimeSurvey RemoteControl2 API",
  "keyword": [
    "limesurvey",
    "laravel-limesurvey",
    "limesurvey-api",
    "json-RPC",
    "limesurvey-jsonRPC"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "neilherbertuk",
      "email": "neil@ea3.co.uk"
    },
      {
      "name": "du-lei",
      "email": "duleijob@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "neilherbertuk\\laravellimesurveyapi\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "require": {
    "weberhofer/jsonrpcphp": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "weberhofer/jsonrpcphp": "^2.2"
  },
  "extra": {
    "laravel": {
      "providers": [
        "neilherbertuk\\laravellimesurveyapi\\LaravelLimesurveyApiProvider"
      ],
      "aliases": {
      	"LimeSurveyAPI": "neilherbertuk\\laravellimesurveyapi\\Facades\\LimeSurveyAPI"
      }
    }
  }
}
